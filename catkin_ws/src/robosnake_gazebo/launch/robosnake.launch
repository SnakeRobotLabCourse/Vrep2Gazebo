<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find robosnake_gazebo)/worlds/robosnake.world"/>
    <!-- more default parameters can be changed here -->
  </include>
  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_sdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find robosnake_description)/sdf/11segment.sdf -sdf -z 1 -model 11-segment" />
</launch>
